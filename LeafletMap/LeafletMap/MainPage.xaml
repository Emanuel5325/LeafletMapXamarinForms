<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:map="clr-namespace:LeafletMap.CustomRender"
             x:Class="LeafletMap.MainPage">
    <ContentPage.Content>
        <StackLayout>
            <map:IWebViewCustom x:Name="webView" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" HeightRequest="100"/>
            <StackLayout>
                <Button x:Name="Send" Text="Mostrar" Clicked="Send_Clicked"/>
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
